{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_NHOST_BACKEND_URL": "${VITE_NHOST_BACKEND_URL}",
    "VITE_API_BASE_URL": "${VITE_API_BASE_URL}"
  }
}
