    "lodash": "^4.17.23",
    "passport": "^0.7.0",
    "passport-openidconnect": "^0.1.2",
    "@node-saml/passport-saml": "^4.0.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.11.3",
    "ws": "^8.19.0",
    "zod": "^3.23.8",
    "express-rate-limit": "^8.2.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^7.1.4"
