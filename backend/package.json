{
  "name": "teachmo-backend",
  "version": "0.1.0",
  "description": "Teachmo backend API server",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "migrate": "node migrate.js",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "snapshot:daily": "node jobs/snapshotDailyMetrics.js 14",
    "snapshot:hourly": "node jobs/snapshotHourlyMetrics.js 48",
