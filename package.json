    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint src/backend src/observability src/api/base44Client.js src/hooks/useUserRole.ts src/components/shared/ProtectedRoute.tsx src/components/shared/AdminRoleGuard.tsx src/components/hooks/useStore.ts src/components/hooks/useUser.jsx src/components/hooks/useDashboardData.jsx src/components/messages/ChatWindow.jsx src/lib/utils.ts src/main.tsx src/pages/index.jsx --ext .js,.jsx,.ts,.tsx",
    "lint:barrel": "eslint src --ext .js,.jsx,.ts,.tsx --config eslint.barrel.config.js",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:backend": "jest --config jest.backend.config.cjs",
    "test:watch": "jest --watch",
    "test:a11y": "jest --testPathPattern=a11y",
    "backend:check": "node --check backend/routes/ops.js",
    "check:size": "size-limit",
    "loadtest:systemHealth": "node scripts/loadtest/systemHealth.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview": "vite preview",
    "prepare": "husky install",
    "e2e": "playwright test",
    "e2e:ops": "playwright test --grep @launch-gates",
    "e2e:a11y": "PLAYWRIGHT_WEB_SERVER=true playwright test tests/e2e/a11y.spec.ts",
    "e2e:ui": "playwright test --ui",
    "prebuild": "node scripts/generate-build-meta.mjs",
    "preflight:example": "node scripts/preflight-env.mjs --example",
    "preflight:env": "node scripts/preflight-env.mjs",
    "start": "vite preview --host 0.0.0.0 --port ${PORT:-4173}",
    "release:info": "node scripts/generate-build-meta.mjs && node -e \"console.log(require(\\\"./src/generated/buildMeta.ts\\\"))\"",
    "smoke:hasura-permissions": "node scripts/hasura-permissions-smoke.mjs"
